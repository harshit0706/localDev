<apex:page controller="apexController">
    <apex:pageBlock >
        <apex:pageBlockTable value="{!acts}" var="a">
            <apex:column value="{!a.Name}"/>
            <apex:column value="{!a.phone}"/>
            <apex:column value="{!a.type}"/>
            <apex:column value="{!a.industry}"/>
        
        </apex:pageBlockTable>
    </apex:pageBlock>
    
</apex:page>